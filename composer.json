{
    "name": "jeckel/codeception-gherkin-helpers",
    "description": "A collection of Codeception Helpers for Gherkin BDD tests",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien MERCIER-ROJAS",
            "email": "julien@jeckel-lab.fr"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Jeckel\\GherkinHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Jeckel\\GherkinHelper\\": "src/"
        }
    },
    "require": {
        "codeception/codeception": "^2.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.5",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    }
}
